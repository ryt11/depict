<%= form_for :session do |f| %>

<div class="form">
  <div class="row">
    <div class="col-md-4 form-spacing">
      <%= f.text_field :username, placeholder: 'Username'%>
    </div>
    <div class="col-md-4 form-spacing">
      <%= f.password_field :password, placeholder: 'Password' %>
    </div>
    <div class="col-md-4 form-spacing">
      <%= f.submit "Log In", class: "depict-btn btn" %>
    </div>
  </div>
</div>
<%end%>
